---
import NixStore from "../components/NixStore.astro";
import "../styles/global.css";
import CenteredBody from "../components/CenteredBody.astro";
import { Code } from "astro:components";
const nix_code = `{
  name = "Elissa";
  age = 19;

  about = ''
    First year compsci student
  '';

  interests = [
    "nix"
    "FOSS"
    "self hosting"
    "coding"
  ];

  languages = [
    "rust"
    "c++"
    "nix"
    "bash"
  ];

  socials = {
    github = "blahai";
    gitlab = "blahai";
    bsky = "elissa.moe";
    discord = 381494697073573899;
  };
